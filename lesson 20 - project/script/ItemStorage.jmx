        <hashTree>
          <JSR223Timer guiclass="TestBeanGUI" testclass="JSR223Timer" testname="JSR223 Timer" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">/**
* PACING END
* Calculate the pacing and apply // return!
* 
*/

def d = new Date()
def percent = vars.get(&quot;PACING&quot;).toInteger()*0.15
def pacingInt = vars.get(&quot;PACING&quot;).toInteger()
def pacingMin = (pacingInt - percent).intValue()
def pacingMax = (pacingInt + percent).intValue()
def randomPacing = new Random().nextInt((pacingMax - pacingMin) + 1) + pacingMin
try {
	
    def pacing =  new Long(randomPacing) // get the required pacing value from jmeter variable.
    String startTime = vars.get(&quot;pacingStartTime&quot;) // get the start time which was set in the beginning of the loop
    def diff = d.getTime() - Long.parseLong(startTime) // current time minus start time
    def sleep = pacing &gt; diff ? pacing - diff : 0 // logic for sleep time
 //   log.info(&quot;[ Pacing: &quot;+randomPacing+&quot;ms, Remaining time: ${sleep}ms ]&quot;)
    return sleep 
}
catch (Exception e) {
    return 1000
    log.warn(&quot;[ Pacing: Failed to calculate pacing ]&quot;, e)
    throw e;
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Timer>
